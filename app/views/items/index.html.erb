<h1>items#index</h1>
<ul>Menu:
	<% @items.each do |item| %>
		<li><%= item.item_name %></li>
		<li>
		<%= form_for @order do |f| %>
			<%= f.label :total %>
			<%= f.number_field :total %>
			<%= f.label :contact_information %>
			<%= f.text_area :contact_information %>
			<%= f.label :delivery_address %>
			<%= f.text_area :delivery_address %>
			<%= f.submit %>
		<% end %>
		</li>
	<% end %>
</ul>